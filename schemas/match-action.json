{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "type": "object",
  "required": [
    "id",
    "gmtTimestamp",
    "matchTime",
    "team",
    "action"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "gmtTimestamp": {
      "type": "number"
    },
    "matchTime": {
      "type": "number",
      "minimum": 0
    },
    "team": {
      "enum": [
        "LOCAL",
        "VISITOR"
      ]
    },
    "action": {
      "enum": [
        "GOAL",
        "TIMEOUT",
        "WARNING",
        "SUSPENSION",
        "DISQUALIFICATION",
        "MATCH_START",
        "MATCH_END",
        "START_SEVEN_PLAYERS",
        "END_SEVEN_PLAYERS",
        "OTHER"
      ]
    },
    "payload": {}
  }
}